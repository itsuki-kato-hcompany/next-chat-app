{
  "name": "nestjs_app",
  "dockerComposeFile": "../../docker-compose.yml",
  "service": "backend",
  "workspaceFolder": "/usr/src/app",
  "shutdownAction": "none",
  "remoteUser": "node",
  "customizations": {
    "vscode": {
      "extensions": [
        "prisma.prisma",
        "esbenp.prettier-vscode",
        "Postman.postman-for-vscode",
        "rangav.vscode-thunder-client@2.21.15",
        "GitHub.copilot",
        "GitHub.copilot-chat"
      ],
      "settings": {
        "prettier.prettierPath": "./node_modules/prettier"
      }
    }
  }
}
